# Настройки базы данных для разработки
POSTGRES_USER=zabbix
POSTGRES_PASSWORD=zabbix_pwd
POSTGRES_DB=zabbix

# Настройки Zabbix Server
ZBX_DEBUGLEVEL=3
ZBX_CACHESIZE=64M
ZBX_HISTORYCACHESIZE=32M
ZBX_HISTORYINDEXCACHESIZE=8M
ZBX_TRENDCACHESIZE=8M
ZBX_VALUECACHESIZE=32M

# Порты для разработки
ZABBIX_SERVER_PORT=10051
ZABBIX_WEB_HTTP_PORT=8081
ZABBIX_WEB_HTTPS_PORT=8443
ZABBIX_JAVA_GATEWAY_PORT=10052
ZABBIX_AGENT_PORT=10050
POSTGRES_PORT=5432

# Настройки веб-интерфейса
PHP_TZ=Europe/Moscow
ZBX_SERVER_NAME=Zabbix Development
NGINX_CLIENT_MAX_BODY_SIZE=16m

# Настройки Java Gateway
ZBX_JAVA_GATEWAY_DEBUG_LEVEL=info
ZBX_JAVA_GATEWAY_TIMEOUT=3

# Настройки Agent
ZBX_HOSTNAME=Zabbix Development Server
ZBX_AGENT_DEBUG_LEVEL=3
ZBX_PASSIVE_ALLOW=true
ZBX_ACTIVE_ALLOW=true

# Контейнеры
POSTGRES_CONTAINER_NAME=zabbix-postgres-dev
ZABBIX_SERVER_CONTAINER_NAME=zabbix-server-dev
ZABBIX_WEB_CONTAINER_NAME=zabbix-web-dev
ZABBIX_JAVA_GATEWAY_CONTAINER_NAME=zabbix-java-gateway-dev
ZABBIX_AGENT_CONTAINER_NAME=zabbix-agent-dev
NGINX_CONTAINER_NAME=zabbix-nginx-dev